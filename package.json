{
  "name": "functional-concepts-ts",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "typ-ramda": "typings install dt~ramda --save",
    "typ-chai": "typings install dt~chai --global --save",
    "typ-mocha": "typings install dt~mocha --global --save",
    "typings": "npm run typ-ramda && npm run typ-chai && npm run typ-mocha",
    "test": "mocha --compilers ts:ts-node/register --recursive test",
    "lint": "tslint --format verbose 'src/**/*.ts'"
  },
  "author": "Oussama BEN BRAHIM",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "ts-node": "^1.7.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.10",
    "typings": "^2.0.0"
  },
  "dependencies": {
    "ramda": "^0.22.1"
  }
}
